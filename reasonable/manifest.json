{
  "name": "reasonable",
  "version": "1.1.3",
  "description": "For a site called reason, you'd think...",
  "icons": {
    "16": "/image/icon_16.png",
    "32": "/image/icon_32.png",
    "48": "/image/icon_48.png",
    "128": "/image/icon_128.png"
  },
  "page_action": {
    "default_icon": "/image/icon_16.png",
    "default_title": "reasonable",
    "default_popup": "options.html"
  },
  "options_page": "options.html",
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["http://reason.com/*"],
      "css": ["/style/reasonable.css"],
      "js": ["/script/jquery-1.4.4.min.js", "/script/reasonable.js"]
    }
  ],
  "permissions": [
    "tabs",
    "http://www.brymck.com/reasonable/"
  ]
}